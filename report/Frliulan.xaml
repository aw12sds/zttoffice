<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ztoffice.report"
             xmlns:dxpdf="http://schemas.devexpress.com/winfx/2008/xaml/pdf" x:Class="ztoffice.report.Frliulan"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <dxpdf:PdfViewerControl HorizontalAlignment="Left" Height="294" Margin="-2,7,-1,-1" VerticalAlignment="Top" Width="303" RenderTransformOrigin="0.5,0.5">
            <dxpdf:PdfViewerControl.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="0.082"/>
                    <TranslateTransform/>
                </TransformGroup>
            </dxpdf:PdfViewerControl.RenderTransform>
        </dxpdf:PdfViewerControl>

    </Grid>
</UserControl>
